# Venice API Configuration

# API Endpoint
VENICE_API_BASE_URL=https://api.venice.ai/v1

# API Key (use environment variable for security)
# VENICE_API_KEY=your_api_key_here

# Model selection
VENICE_MODEL_NAME=venice-medium

# Request timeout in seconds
VENICE_REQUEST_TIMEOUT=60

# Maximum number of retries for failed requests
VENICE_MAX_RETRIES=3

# Temperature setting for model responses
VENICE_TEMPERATURE=0.7

# Maximum tokens to generate
VENICE_MAX_TOKENS=2048

# Additional headers to include in requests
VENICE_CUSTOM_HEADERS=

# Enable/disable caching of responses
VENICE_ENABLE_CACHING=true

# Cache TTL in seconds
VENICE_CACHE_TTL=3600